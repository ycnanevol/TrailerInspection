import Foundation

public enum LightType {
    case LED
    case Filament
}

extension LightType : Printable {
    public var description: String {
        switch self {
        // TODO(jjiang): to get the printable values from NSLocalizedString
        case .LED : return "LED"
        case .Filament : return "Filament"
        }
    }
}