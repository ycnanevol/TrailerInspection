import Foundation

public enum TrailerType {
    case Van, TCU, Unknown
}

extension TrailerType : Printable {
    
    public var description : String {
        switch self {
        case .Van: return "Van"
        case .TCU: return "TCU"
        case .Unknown: return "Unknown"
        }
    }
    
}
